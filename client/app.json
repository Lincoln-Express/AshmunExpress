{
  "expo": {
    "name": "Ashmun Express",
    "slug": "ashmun-express",
    "description": "",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#F57C00",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F57C00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.learningexpress.ashmunexpress",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.learningexpress.ashmunexpress",
      "versionCode": 1
    },
    "userInterfaceStyle": "automatic",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "svg"]
    }
  }
}
